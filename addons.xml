<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.cocina"
       name="recetas"
       version="0.0.2"
       provider-name="luvf">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.liveresolver" version="0.1.24" optional="true"/>
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" optional="true" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
     <import addon="script.module.livestreamer" optional="true"/>
     <import addon="script.module.pyamf" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>

  <extension point="xbmc.addon.metadata">
    <summary>Creado por luvf</summary>
    <description>comer bien</description>
    <platform>all</platform>
  </extension>
</addon>


<addon id="repository.kodi.luvarofe" name="kodi.luvarofe.repo" version="1.1.0" provider-name="kodiluvarofe">
	<extension point="xbmc.addon.repository" name="luvarofe official Repo">
		<info compressed="false">https://raw.githubusercontent.com/luvarofe/MyRepo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/luvarofe/MyRepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/luvarofe/MyRepo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Kodi-luvarofe official Repo</summary>
   		<description lang="en">Instala los Addons de Kodi luvarofe.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
